Country = ("1.Australia", "2.America", "3.Japan", "4.Canada", "5.England")

for i in range(len(Country)):
  print(Country[i])

print()
Ans1 = int(input("What 3 country you like the most ? first : "))
Ans2 = int(input("What 3 country you like the most ? second : "))
Ans3 = int(input("What 3 country you like the most ? third : "))

print()
print(Country[Ans1-1],Country[Ans2-1], Country[Ans3-1], end="")
